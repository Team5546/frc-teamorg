  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "start": "concurrently \"node src/server/index.js\" \"npm run db\"",
    "build": "webpack --mode production",
    "client": "webpack-serve --config ./webpack.config.js --hotClient",
    "db": "mongod --dbpath ~/db",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run db\""
